package pila123;

public class Libro {

	private String nombreLibro;
	private String autor;
	
	public Libro(){
		this.nombreLibro="";
		this.autor="";
		
	}
	
	public Libro(String nombreLibro, String autor){
		this.nombreLibro=nombreLibro;
		this.autor=autor;
	}
	
	public String getTitulo(){
		return nombreLibro;
	}
	public String getAutor(){
		return autor;
	}
	
	public void 
	
}
